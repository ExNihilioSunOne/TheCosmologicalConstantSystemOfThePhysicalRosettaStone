\[
\begin{aligned}
w_1 &= 0.326 & \text{(453.301/144)} \\
w_2 &= 0.112 & \text{(745.269/237.166)} \\
w_3 &= 0.357 & \text{(386.284/123)} \\
w_4 &= 0.005 & \text{(482.631/153.6)} \\
